<?xml profile="1.0" encoding="iso-8859-1"?>
<xml>
  <pixelshader name="color3d">
    <source>color_ambient.ps</source>
    <entry>ps_main</entry>
    <profile>ps_4_0</profile>
	<constants>
	  <buffer>
	    <float3 name="AmbientDown">
		  <default>0.1, 0.1, 0.1</default>
		</float3>
		<float1/>
	    <float3 name="AmbientUp">
		  <default>0.9, 0.9, 0.9</default>
		</float3>
		<float1/>
	  </buffer>
	</constants>
  </pixelshader>
  <vertexshader name="color3d">
    <source>color_ambient.vs</source>
    <entry>vs_main</entry>
    <profile>vs_4_0</profile>
	<constants>
	  <buffer>
	    <matrix name="world" count="1"/>
	    <matrix name="view" count="1"/>
	    <matrix name="projection" count="1"/>
	  </buffer>
	</constants>
    <vd>
      <element name="Position" type="Float3"/>
  	  <element name="Diffuse" type="Color"/>
	  <element name="Normal" type="Float3"/>
    </vd>
  </vertexshader>
</xml>
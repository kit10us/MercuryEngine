<modify texture="0,mountain_tile"/>
<modify texture="1,grass_tile"/>
<modify texture="2,Mountain_Mix"/>
<modify pixelshader="Mix Shader 2"/>
<texturemap tex="tex1" ul="0,0" dr="5,5"/>
<texturemap tex="tex2" ul="0,0" dr="1,1"/>
<texturemap tex="tex3" ul="0.5,0.25" dr="0.75,0.5"/>

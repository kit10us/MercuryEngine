<!-- Down Right -->
<terra name="Mountain08" size="10,10" fvf="XYZ|NORMAL|DIFFUSE|TEX3|TEXCOORDSIZE2(0)|TEXCOORDSIZE2(1)|TEXCOORDSIZE2(2)" rows="30" columns="30" constant="2.5">
<heightmap texture="mountain_h" colorop="0,10,0,0" ul="0,0" dr="0.25,0.25"/>
<modify texture="0,mountain_tile"/>
<modify texture="1,grass_tile"/>

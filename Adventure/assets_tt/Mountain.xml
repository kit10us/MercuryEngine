<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- Note: Index in vertex definition is arbatrary, and if included (which is suggested) is used as checking AND should start 0-->
<pack>

<texture name="mountain_tile" source="images/mountain_tile.bmp"/>
<texture name="mountain_mix" source="images/mountain_mix.bmp"/>
<texture name="mountain_s" source="images/mountain_s.bmp"/>
<texture name="mountain_h" source="images/mountain_h.bmp" lockable="lockable" norender="norender"/>
<texture name="mountain_sh" source="images/mountain_sh.bmp" lockable="lockable" norender="norender"/>

<!-- Center -->
<terra name="Mountain00" size="10,10" fvf="XYZ|NORMAL|DIFFUSE|TEX3|TEXCOORDSIZE2(0)|TEXCOORDSIZE2(1)|TEXCOORDSIZE2(2)" rows="30" columns="30" constant="2.5">
<heightmap texture="mountain_h" colorop="0,10,0,0" ul="0.25,0.25" dr="0.5,0.5"/>
<modify texture="0,mountain_tile"/>
<modify texture="1,grass_tile"/>
<modify texture="2,Mountain_Mix"/>
<modify pixelshader="Mix Shader 2"/>
<texturemap tex="tex1" ul="0,0" dr="5,5"/>
<texturemap tex="tex2" ul="0,0" dr="1,1"/>
<texturemap tex="tex3" ul="0.25,0.25" dr="0.5,0.5"/>
</terra>

<!-- Up -->
<terra name="Mountain01" size="10,10" fvf="XYZ|NORMAL|DIFFUSE|TEX3|TEXCOORDSIZE2(0)|TEXCOORDSIZE2(1)|TEXCOORDSIZE2(2)" rows="30" columns="30" constant="2.5">
<heightmap texture="mountain_h" colorop="0,10,0,0" ul="0.25,0.5" dr="0.5,0.75"/>
<modify texture="0,mountain_tile"/>
<modify texture="1,grass_tile"/>
<modify texture="2,Mountain_Mix"/>
<modify pixelshader="Mix Shader 2"/>
<texturemap tex="tex1" ul="0,0" dr="5,5"/>
<texturemap tex="tex2" ul="0,0" dr="1,1"/>
<texturemap tex="tex3" ul="0.25,0.5" dr="0.5,0.75"/>
</terra>


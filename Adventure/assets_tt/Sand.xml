<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- Note: Index in vertex definition is arbatrary, and if included (which is suggested) is used as checking AND should start 0-->
<pack>

<texture name="Sand_tile" source="images/Sand_tile.dds"/>
<texture name="Sand_mix" source="images/Sand_mix.bmp"/>
<texture name="Sand_h" source="images/Sand_h.bmp" lockable="lockable" norender="norender"/>

<!-- Center -->
<terra name="Sand00" size="10,10" fvf="XYZ|NORMAL|DIFFUSE|TEX3|TEXCOORDSIZE2(0)|TEXCOORDSIZE2(1)|TEXCOORDSIZE2(2)" rows="40" columns="40" constant="2.5">
<heightmap texture="Sand_h" colorop="0,0.5,0,0" ul="0.25,0.25" dr="0.5,0.5"/>
<modify texture="0,Sand_tile"/>
<modify texture="1,grass_tile"/>
<modify texture="2,Sand_Mix"/>
<modify pixelshader="Mix Shader 1"/>
<texturemap tex="tex1" ul="0,0" dr="1,1"/>
<texturemap tex="tex2" ul="0,0" dr="5,5"/>
<texturemap tex="tex3" ul="0.25,0.25" dr="0.5,0.5"/></terra>


<texture name="ocean1_h1" source="images/ocean1_h1.bmp" lockable="lockable" norender="norender"/>


<terra name="OceanWater L2" size="10,10" fvf="XYZ|NORMAL|TEX1" rows="10" columns="10" constant="1.2">
<modify texture="0,ocean_sea2"/>
<modify blend="SrcAlpha, InvSrcAlpha"/>
</terra>

<terra name="OceanWater L1" size="10,10" fvf="XYZ|NORMAL|TEX1" rows="10" columns="10" constant="1.5">
<modify texture="0,ocean_sea1"/>
<modify blend="SrcAlpha, InvSrcAlpha"/>
<modify specularpower="20"/>
<modify specular="UnitARGB: 1, 1, 1, 1)"/>
<modify pixelshader="Ocean Shader"/>
<modify vertexshader="Ocean Shader"/>
</terra>

<geometry name="OceanWater" type="group">
<add geometry="OceanWater L2"/>
<add geometry="OceanWater L1"/>
</geometry>

<geometry name="OceanFloor" size="10,10" fvf="XYZ|NORMAL|TEX1" rows="1" columns="1" constant="-2.4">
<modify texture="0,ocean_bottom"/>
</geometry>



<xml>
<title>Adventure DX11</title>
<renderer>../../renderers/MercuryEngineDX11.dll</renderer>
<failuresAsCritial>true</failuresAsCritial>
<logfile>log.txt</logfile>
<assets>shaders/</assets>
<assets>../assets_common/</assets>
<assets>../assets_common/models/</assets>
<assets>../assets_common/images/</assets>
<assets>../assets_common/scripts/</assets>
<assets>../assets_common/effects/</assets>
<display fullscreen="false" x="10" y="10" width="800" height="600"/>
<extension source="../../extensions/DXILua.dll">
	<startup>startup.lua</startup>
</extension>
<extension source="../../extensions/DAELoader.dll">
	<color name="color_ambient" source="Color_Ambient.effect"/>
	<texture name="textured_ambient" source="Textured_Ambient.effect"/>
</extension>
<extension source="../../extensions/ASELoader.dll">
	<textureps name="textured_ambient" source="textured_ambient.xml"/>
	<texturevs name="textured_ambient" source="textured_ambient.xml"/>
</extension>
<extension source="../../extensions/DXIKeyboard.dll"/>
<extension source="../../extensions/DXIGamepad.dll"/>
<extension source="../../extensions/MEEditor.dll"/>
<motivator source="Keyboard" subSource="0" type="ButtonPressed" input="Escape" command="Quit"/>
</xml>